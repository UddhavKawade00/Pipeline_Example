pipeline {
    agent any
    stages {
        stage('Compile') {
            steps {
                sh 'javac Test.java'
            }
        }
        stage('Run') {
            steps {
                sh 'java Test'
            }
        }
    }
    post {
        success {
            echo 'Pipeline executed successfully.'
        }
        failure {
            echo 'Pipeline execution failed.'
        }
    }
}
